<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
                xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="richTextEditor"
                nsURI="http://www.example.org/richTextEditor" nsPrefix="richTextEditor">
    <eClassifiers xsi:type="ecore:EClass" name="RichTextEditorSettings">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
            <details key="documentation" value="Represents the configuration settings for a rich text editor, e.g., which editor type to use."/>
        </eAnnotations>
        <eStructuralFeatures xsi:type="ecore:EAttribute" name="editor" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
